{
  "name": "social-media",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "dotenv -- nest start",
    "start:dev": "dotenv -- nest start --watch",
    "start:debug": "dotenv -- nest start --debug --watch",
    "start:prod": "dotenv -v NODE_ENV=production -- node dist/main",
    "test": "dotenv -v NODE_ENV=test -- vitest run",
    "test:watch": "dotenv -v NODE_ENV=test -- vitest watch",
    "test:cov": "yarn test --coverage",
    "test:e2e": "yarn test --config test/vitest.config.ts",
    "test:all": "yarn ts:check && yarn test && yarn test:e2e && yarn lint",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "ts:check": "tsc --noEmit",
    "find-deadcode": "ts-prune -p tsconfig.json",
    "typeorm": "dotenv -- ts-node --require tsconfig-paths/register --require ./tsconfig.build.json ./node_modules/typeorm/cli",
    "migration": "yarn typeorm --dataSource ./src/database/typeorm-data-source.ts",
    "migration:generate": "yarn migration migration:generate --pretty",
    "migration:create": "yarn typeorm migration:create",
    "migration:show": "yarn migration migration:show",
    "migration:run": "yarn migration migration:run",
    "migration:revert": "yarn migration migration:revert"
  },
  "dependencies": {
    "@adminjs/express": "^5.0.1",
    "@adminjs/nestjs": "^5.0.1",
    "@adminjs/typeorm": "^4.0.0",
    "@devniel/nestjs-typeorm-testing": "^0.1.5-alpha",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.4.4",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.4.4",
    "adminjs": "^6.7.2",
    "bcrypt": "^5.0.1",
    "better-sqlite3": "^7.5.1",
    "dotenv-cli": "^5.1.0",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "mysql2": "^2.3.3",
    "nestjs-joi": "^1.7.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.6",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@swc/core": "^1.3.27",
    "@types/bcrypt": "^5.0.0",
    "@types/chance": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/node": "^16.11.10",
    "@types/passport-jwt": "^3.0.6",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitest/coverage-c8": "^0.27.2",
    "chance": "^1.1.9",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "10.7.0",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "^3.10.1",
    "typescript": "4.5.2",
    "typescript-transform-paths": "^3.3.1",
    "unplugin-swc": "^1.3.2",
    "vitest": "^0.27.2"
  }
}
